{
  "name": "vscode-ab-st",
  "displayName": "vscode-ab-st",
  "description": "VS Code Language Support for Allen Bradley Structured Text",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "abst",
      "aliases": ["AB ST", "abst"],
      "extensions": [".st",".scl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "abst",
      "scopeName": "source.abst",
      "path": "./syntaxes/abst.tmLanguage.json"
    }]
  }
}
